@article{esner_presence_2011-2,
  title = {{Presence in Absence: The Empty Studio as Self-Portrait}},
  shorttitle = {{Presence in Absence}},
  author = {Esner, Rachel},
  year = {2011},
  volume = {56},
  pages = {79--100},
  issn = {2366-0740, 0044-2186},
  doi = {10.28937/1000106179},
  abstract = {Seit der Epoche der Romantik bringen viele der Diskurse, die k\"unstlerische Kreativit\"at betreffen, den K\"unstler und seinen Arbeitsraum zusammen: Die Werkstatt wird als ein Spiegel des K\"unstlers und seines Werks angesehen, als ein Heiligtum, ein sozialer Raum oder eine Ausstellungsfl\"ache. Ein popul\"arer bildlicher Topos der Zeit war die Darstellung des leeren Ateliers. Der vorliegende Beitrag versteht das Bild des leeren Ateliers im 19. Jahrhundert als ein Selbstportr\"at des K\"unstlers. Er untersucht, wie die Darstellung des Raums und seiner Objekte dem Gem\"alde die Pr\"asenz des K\"unstlers einschreiben und sein bzw. ihr k\"unstlerisches~ Selbstverst\"andnis innerhalb der sich ver\"andernden k\"unstlerischen und sozialen Strukturen der Epoche wiedergeben.{$<$}br{$><$}br{$>$}From the Romantic era onward, many of the discourses surrounding artistic creativity have merged the artist and his working space: the place of work is viewed as the mirror of the man and his oeuvre, a sanctuary, a social or an exhibition space. A popular topos in this context was the view of the empty studio. This paper explores the 19th-century empty studio image as a self-portrait of the artist. It examines how the depictions of the space and its objects work to inscribe the artist's presence, and to express his or her~ artistic self-conception and identity within the changing artistic-social structures of the period.},
  file = {/Users/ilsunmoon/Zotero/storage/B78Y29G5/Esner - 2011 - Presence in Absence The Empty Studio as Self-Port.pdf},
  journal = {Zeitschrift f\"ur \"Asthetik und Allgemeine Kunstwissenschaft},
  language = {de},
  number = {2}
}

